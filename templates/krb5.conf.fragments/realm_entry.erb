
  <%= @upcase_name %> = {
    <%- if @kdc -%>
      <%- if @kdc.kind_of?(Array) -%>
        <%- Array(@kdc).each do |server| -%>
      kdc            = <%= server %>
        <%- end -%>
      <%- else -%>
      kdc            = <%= @kdc %>
      <%- end -%>
    <%- end -%>
    <%- if @master_kdc -%>
    master_kdc     = <%= @master_kdc %>
    <%- end -%>
    <%- if @admin_server -%>
    admin_server   = <%= @admin_server %>
    <%- end -%>
    <%- if @default_domain -%>
    default_domain = <%= @default_domain %>
    <%- end -%>
    <%- if @kpasswd_server -%>
    kpasswd_server = <%= @kpasswd_server %>
    <%- end -%>
  }
