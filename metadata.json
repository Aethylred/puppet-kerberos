{
  "name": "Aethylred-kerberos",
  "version": "0.2.0",
  "author": "Aethylred",
  "license": "Apache-2.0",
  "summary": "A module for installing and managing kerberos",
  "source": "https://github.com/Aethylred/puppet-kerberos",
  "project_page": "https://github.com/Aethylred/puppet-kerberos",
